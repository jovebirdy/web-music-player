	//alert("${musicname}");
	//获取每页的数据
	function getPage(page,k){
		var ps = Number(document.getElementById("pageSize").value);
		var tc = Number(document.getElementById("counts").value);
		var sn = "${singername == null? '':singername}";
		//alert(tc);
		//alert(sn);
		$.ajax({  
	        type : "POST",  
	        url : $('#contextPath').val() + "/singer/ajax_operation" ,  
	        dataType:"json",
	        cache : false,  
	        data : {  
	            pageIndex: page,
	            pageSize: ps,
	            totalPage:tc,
	            singerName:sn
	        },  
	        async : false,  
	        error : function() {  
	            alert("网络异常！");  
	        },  
	        success : function(data) { 
	        	var html = "";
	        	html += '<table class="table table-striped b-t b-light text-sm"><thead><tr ><th class="text-center">编号  </th><th class="text-center">歌手名</th><th class="text-center">封面</th><th class="text-center">上传时间</th><th class="text-center">点击次数</th><th class="text-center">简介</th><th class="text-center" width="15%">操作</th></tr></thead><tbody>';
	    		for(var i=0;i<data.length;i++){	
	    			html += '	<tr>';
	    			html += '          <td>' + data[i].id + '</td>';
	    			html += '         <td>' + data[i].singername + '</td>';
	    			html += ' <td>' + data[i].img + '</td>';
	    			html += '        <td>' + format(data[i].uploadtime, 'yyyy年MM月dd日 HH时mm分ss秒') + '</td>';
	    			html += '      <td>' + data[i].counts + '</td>';
		            html += '        <td>' + data[i].introduction + '</td>';
		            html += '  <td>';
		            html += '	<a href="javascript:void(0)" onclick="updateSinger_get(' + data[i].id + ')" class="active" ><i style="font-size:15px;" class="fa fa-edit"></i>编辑</a>';
		            html += '  	&nbsp;&nbsp;&nbsp;';
		            html += '  	<a href="javascript:void(0)" onclick="delete_singer(' + data[i].id + ')" class="active" data-toggle="modal" data-target="#myModal2"><i style="font-size:15px;" class="fa fa-times"></i>删除</a>';
		            html += '   </td>';
		            html += '  </tr>';
		                    									  
	    		}
	    		html += ' </tbody>';
				html += '</table>';
	    		$('#result').html(html);
	        }  
	    });  
	};
	
    
    //alert(format(1396178344662, 'yyyy-MM-dd HH:mm:ss')); 
	
	
</script>
